<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Ytd_Funnel_Opportunities__c</fullName>
    <deprecated>false</deprecated>
    <description>This field stores the count of qualified opp ytd + the count of the qualified opp moved to next month + the count of the qualified opps that are lost ytd.</description>
    <externalId>false</externalId>
    <formula>IF(YTD_Opportunity_Close_Rate__c &gt; 0,YTD_Opportunities_won__c / YTD_Opportunity_Close_Rate__c ,0)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Ytd Funnel Opportunities</label>
    <precision>18</precision>
    <required>false</required>
    <scale>0</scale>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Number</type>
    <unique>false</unique>
</CustomField>
