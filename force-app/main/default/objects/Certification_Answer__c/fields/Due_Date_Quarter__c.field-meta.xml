<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Due_Date_Quarter__c</fullName>
    <deprecated>false</deprecated>
    <description>This formula determines the Quarter of the Due Date for reporting purposes.</description>
    <externalId>false</externalId>
    <formula>TEXT(YEAR( Due_Date__c )) &amp; &quot;Q&quot; &amp; TEXT(FLOOR(((MONTH(Due_Date__c)-1)/3)+1))</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Due Date Quarter</label>
    <required>false</required>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Text</type>
    <unique>false</unique>
</CustomField>
